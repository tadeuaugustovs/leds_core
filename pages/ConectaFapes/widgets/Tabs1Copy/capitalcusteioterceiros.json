  "tableData": "{{\n  getNotasFiscais.data?.data\n    ?.reduce((acc, nota) => {\n      const natureza = nota.natureza_despesa || '-';\n      const valor = nota.valor_liquido || 0;\n\n      const tipoOrcamento = nota.tipoOrcamento || (\n        ['Passagem', 'DiÃ¡ria', 'Hospedagem/AlimentaÃ§Ã£o', 'Cursos'].includes(natureza)\n          ? 'CUSTEIO'\n          : 'CAPITAL'\n      );\n\n      const existente = acc.find(e => e.item === natureza && e.orcamento === tipoOrcamento);\n\n      if (existente) {\n        existente.gasto += valor;\n      } else {\n        acc.push({\n          orcamento: tipoOrcamento,\n          item: natureza,\n          qtde: '-',\n          valorUnitario: '-',\n          valorOrcado: 0,\n          gasto: valor,\n          restante: 0,\n        });\n      }\n\n      return acc;\n    }, [])\n    .map(item => {\n      item.restante = item.valorOrcado - item.gasto;\n      return item;\n    })\n    .sort((a, b) => {\n      const ordem = { 'CAPITAL': 0, 'CUSTEIO': 1, 'TERCEIROS': 2 };\n      return ordem[a.orcamento] - ordem[b.orcamento];\n    })\n}}\n",
