  "tableData": "{{\n  getCapitalCusteioItens.data?.data\n    ?.filter(item => item.projetos?.some(p => p.documentId === appsmith.store.projetoSelecionadoDocumentId))\n    ?.sort((a, b) => {\n      // Ordena: CAPITAL primeiro, depois CUSTEIO\n      if (a.Tipo === \"CAPITAL\" && b.Tipo === \"CUSTEIO\") return -1;\n      if (a.Tipo === \"CUSTEIO\" && b.Tipo === \"CAPITAL\") return 1;\n      return 0;\n    })\n    ?.map(item => ({\n      documentId: item.documentId,\n      tipo: item.Tipo || \"-\",\n      item: item.item || \"-\",\n      total: \"R$ \" + (item.valorTotal || 0).toLocaleString(\"pt-BR\"),\n      gasto: \"R$ \" + (item.GastoTotal || 0).toLocaleString(\"pt-BR\"),\n      restante: \"R$ \" + (item.Restante || 0).toLocaleString(\"pt-BR\")\n    }))\n}}\n",
