  "tableData": "{{\n(() => {\n  const membroSelecionado = Table1?.selectedRow;\n  const todosMembros = getMembros.data?.data || [];\n\n  if (!membroSelecionado || !membroSelecionado.documentId) return [];\n\n  const membro = todosMembros.find(m => m.documentId === membroSelecionado.documentId);\n  const planos = membro?.planos_de_trabalho || [];\n\n  const formatarData = data => {\n    if (!data) return '-';\n    const dateObj = new Date(data);\n    if (isNaN(dateObj)) return '-';\n    return dateObj.toISOString().split('T')[0];\n  };\n\n  const ordem = { \"ATIVA\": 0, \"PLANEJADA\": 1, \"INATIVA\": 2 };\n\n  return planos\n    .sort((a, b) => (ordem[a.statusBolsa] ?? 3) - (ordem[b.statusBolsa] ?? 3))\n    .map(plano => ({\n      documentId: plano?.documentId, // necess√°rio para a query PUT\n      bolsa: plano?.bolsa?.nome || '-',\n      status: plano?.statusBolsa || '-',\n      inicio: formatarData(plano?.inicioBolsa),\n      fim: formatarData(plano?.fimBolsa),\n      motivo: plano?.motivoInatividade || '-'\n    }));\n})()\n}}\n",
