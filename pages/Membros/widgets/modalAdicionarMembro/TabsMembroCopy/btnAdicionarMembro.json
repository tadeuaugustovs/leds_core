  "onClick": "{{\n  (async () => {\n    const nome = appsmith.store.novoMembro?.nome?.trim();\n\n    if (!nome || nome === \"\") {\n      showAlert(\"O nome é obrigatório para cadastrar um membro.\", \"warning\");\n      return;\n    }\n\n    await criarMembro.run({\n      data: {\n        nome: nome\n      }\n    });\n\n    showAlert(\"Membro adicionado com sucesso!\", \"success\");\n    closeModal(\"modalAdicionarMembro\");\n\n    getMembros.run(); // Recarrega a tabela\n  })().catch((err) => {\n    showAlert(\"Erro ao adicionar membro: \" + err.message, \"error\");\n  })\n}}\n",
