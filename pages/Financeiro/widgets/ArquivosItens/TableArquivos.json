  "tableData": "{{\n  (getItemComArquivos.data?.data?.arquivos || []).map(arquivo => {\n    const data = arquivo.createdAt\n      ? new Date(arquivo.createdAt).toLocaleDateString(\"pt-BR\")\n      : '-';\n\n    return {\n      nome: arquivo.name || '-',\n      tipo: arquivo.mime || '-',\n      data,\n      url: `http://10.128.128.20:1337${arquivo.url}` || '-'\n    };\n  })\n}}\n",
