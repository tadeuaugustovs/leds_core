  "tableData": "{{\nArray.isArray(getMembros.data?.data)\n  ? getMembros.data.data.flatMap(membro => {\n      return (membro.planos_de_trabalho || [])\n        .filter(plano => \n          plano?.bolsa?.nome && \n          plano?.bolsa?.valor &&\n          (\n            appsmith.store.projetoSelecionadoDocumentId === \"TODOS\" ||\n            plano?.projetos?.some(proj => proj?.documentId === appsmith.store.projetoSelecionadoDocumentId)\n          )\n        )\n        .map(plano => {\n          const nome = membro.nome || '-';\n          const bolsa = plano.bolsa.nome || '-';\n\n          const valor = typeof plano.bolsa.valor === 'number'\n            ? `R$ ${plano.bolsa.valor.toLocaleString('pt-BR', { minimumFractionDigits: 2 })}`\n            : 'R$ 0,00';\n\n          const inicioRaw = plano.inicioBolsa;\n          const fimRaw = plano.fimBolsa;\n\n          const inicio = Date.parse(inicioRaw)\n            ? new Date(inicioRaw).toLocaleDateString('pt-BR')\n            : '-';\n\n          const fim = Date.parse(fimRaw)\n            ? new Date(fimRaw).toLocaleDateString('pt-BR')\n            : '-';\n\n          return { nome, bolsas: bolsa, inicio, fim, valor };\n        });\n    })\n  : []\n}}\n",
