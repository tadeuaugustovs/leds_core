  "tableData": "{{\n(() => {\n  const dados = getItensFluxoCaixa.data?.data || [];\n\n  const formatado = dados\n    .sort((a, b) => new Date(b.data) - new Date(a.data))\n    .map(item => {\n      const dataObj = new Date(item.data);\n      return {\n        documentId: item.documentId,\n        tipoLinha: \"registro\",\n        ano: dataObj.getFullYear(),\n        mes: dataObj.getMonth(),\n        data: dataObj.toLocaleDateString(\"pt-BR\"),\n        descricao: item.descricao || \"-\",\n        situacao: item.situacao || \"-\",\n        receita: item.receita != null \n          ? \"R$ \" + Number(item.receita).toLocaleString(\"pt-BR\", { minimumFractionDigits: 2 }) \n          : \"-\",\n        despesa: item.despesa != null \n          ? \"R$ \" + Number(item.despesa).toLocaleString(\"pt-BR\", { minimumFractionDigits: 2 }) \n          : \"-\",\n        contacorrente: item.contacorrente != null \n          ? \"R$ \" + Number(item.contacorrente).toLocaleString(\"pt-BR\", { minimumFractionDigits: 2 }) \n          : \"-\"\n      };\n    });\n\n  const resultadoFinal = [];\n  let ultimoAno = null;\n  let ultimoMes = null;\n\n  for (const item of formatado) {\n    const { ano, mes } = item;\n\n    if (ano !== ultimoAno) {\n      resultadoFinal.push({\n        tipoLinha: \"fechamento_ano\",\n        descricao: `Fechamento ${ano}`,\n        data: \"\",\n        situacao: \"\",\n        receita: \"\",\n        despesa: \"\",\n        contacorrente: \"\"\n      });\n      ultimoAno = ano;\n      ultimoMes = null; // força fechamento de mês também\n    }\n\n    if (mes !== ultimoMes) {\n      resultadoFinal.push({\n        tipoLinha: \"fechamento_mes\",\n        descricao: `Fechamento ${(\"0\" + (mes + 1)).slice(-2)}/${ano}`,\n        data: \"\",\n        situacao: \"\",\n        receita: \"\",\n        despesa: \"\",\n        contacorrente: \"\"\n      });\n      ultimoMes = mes;\n    }\n\n    resultadoFinal.push(item);\n  }\n\n  return resultadoFinal;\n})()\n}}\n",
