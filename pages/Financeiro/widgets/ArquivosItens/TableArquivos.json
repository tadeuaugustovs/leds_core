  "tableData": "{{\n  (getItemComArquivos.data?.data?.arquivos || []).map(arquivo => {\n    const dataEnvio = arquivo.createdAt\n      ? new Date(arquivo.createdAt).toLocaleDateString(\"pt-BR\")\n      : '-';\n\n    const dataDocumento = arquivo.dataDocumento\n      ? new Date(arquivo.dataDocumento).toLocaleDateString(\"pt-BR\")\n      : '-';\n\n    const mime = arquivo.mime || '';\n    const tipoArquivo = mime.includes('pdf')\n      ? 'PDF'\n      : `Imagem [${mime.split('/')[1]?.toUpperCase() || 'FORMATO'}]`;\n\n    const tamanhoFormatado = arquivo.size\n      ? (arquivo.size >= 1024\n          ? (arquivo.size / 1024).toFixed(2) + ' MB'\n          : arquivo.size.toFixed(2) + ' KB')\n      : '-';\n\n    return {\n      nome: arquivo.name || '-',\n      formato: tipoArquivo,\n      tamanho: tamanhoFormatado,\n      dataEnvio,\n      dataDocumento,\n      url: `http://10.128.128.20:1337${arquivo.url}` || '-'\n    };\n  })\n}}\n",
