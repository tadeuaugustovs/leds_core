  "searchKey": "",
  "tableData": "{{\n  (appsmith.store.itensSelecionados || []).map(item => {\n    const receita = Number(item.receita) || 0;\n    const despesa = Number(item.despesa) || 0;\n    const restante = Number(item.contacorrente) || 0;\n\n    const data = item.data\n      ? new Date(item.data).toLocaleDateString(\"pt-BR\")\n      : '-';\n\n    return {\n      data,\n      descricao: item.descricao || '-',\n      receita: item.receita != null ? receita.toLocaleString(\"pt-BR\", { style: \"currency\", currency: \"BRL\" }) : '-',\n      despesa: item.despesa != null ? despesa.toLocaleString(\"pt-BR\", { style: \"currency\", currency: \"BRL\" }) : '-',\n      contacorrente: item.contacorrente != null ? restante.toLocaleString(\"pt-BR\", { style: \"currency\", currency: \"BRL\" }) : '-',\n      situacao: item.situacao || '-'\n    };\n  })\n}}\n",
