  "searchKey": "",
  "tableData": "{{\n  Array.isArray(getNotasFiscais.data?.data)\n    ? getNotasFiscais.data.data\n        .filter(nota => {\n          if (!appsmith.store.projetoSelecionado?.documentId) return true; // Mostra tudo se nada selecionado\n          return nota?.projetos?.some(\n            p => p.documentId === appsmith.store.projetoSelecionado.documentId\n          );\n        })\n        .map(nota => ({\n          id: nota.id,\n          data: nota.data_pagamento\n            ? moment(nota.data_pagamento).format(\"DD/MM/YYYY\")\n            : \"Sem data\",\n          natureza: nota.natureza_despesa || \"Não informada\",\n          pagamento: nota.forma_pagamento || \"Não informada\",\n          numero: nota.numero_documento || \"-\",\n          valor:\n            nota.valor_liquido != null\n              ? `R$ ${Number(nota.valor_liquido).toLocaleString(\"pt-BR\", {\n                  minimumFractionDigits: 2,\n                })}`\n              : \"R$ 0,00\",\n          projetos: (nota.projetos || [])\n            .map(p => p.nome)\n            .join(\", \") || \"Sem projeto\",\n        }))\n    : []\n}}\n",
