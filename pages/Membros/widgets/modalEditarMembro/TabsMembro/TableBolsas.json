  "tableData": "{{ \n  Array.isArray(getPlanoDeTrabalho.data?.data)\n    ? getPlanoDeTrabalho.data.data\n        .filter(plano => plano?.membro?.documentId === appsmith.store.documentIdSelecionado)\n        .sort((a, b) => {\n          const ordem = { \"ATIVA\": 0, \"PLANEJADA\": 1, \"INATIVA\": 2 };\n          return (ordem[a.statusBolsa] ?? 3) - (ordem[b.statusBolsa] ?? 3);\n        })\n        .map(plano => {\n          const bolsa = plano?.bolsa ?? {};\n          const financiador = bolsa?.financiador ?? {};\n          const projetos = plano?.projetos ?? [];\n\n          return {\n            bolsa: bolsa.nome || '-',\n            valor: bolsa.valor || 0,\n            inicio: plano.inicioBolsa || '-',\n            fim: plano.fimBolsa || '-',\n            status: plano.statusBolsa || '-',\n            motivo: plano.motivoInatividade || '-',\n            projeto: projetos.map(p => p.nome).join(', ') || '-',\n            financiador: financiador.nome || '-'\n          };\n        })\n    : []\n}}\n",
